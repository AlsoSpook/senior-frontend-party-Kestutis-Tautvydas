{
  "name": "senior-frontend-party",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@types/jest": "24.0.9",
    "@types/node": "11.9.6",
    "@types/react": "16.8.6",
    "@types/react-dom": "16.8.2",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.3.1",
    "emotion": "^10.0.7",
    "react": "^16.8.3",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.8.3",
    "react-emotion": "^10.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "typescript": "3.3.3333"
  },
  "scripts": {
    "prestart": "npm install",
    "start": "SET NODE_ENV=development & webpack-dev-server --mode development",
    "prebuild": "rimraf dist",
    "build": "SET NODE_ENV=production & webpack --mode production --progress",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage",
    "lint": "npm run lint:ts",
    "lint:ts": "tslint -p . --exclude \"src/**/*.json\""
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/react-custom-scrollbars": "^4.0.5",
    "@types/react-router-dom": "^4.3.1",
    "@types/webpack": "^4.4.25",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "css-hot-loader": "^1.4.3",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "jest-dom": "^3.1.2",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react-testing-library": "^6.0.0",
    "sass-loader": "^7.1.0",
    "tslint": "^5.13.1",
    "tslint-config-airbnb": "^5.11.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|css|scss)$": "<rootDir>/util/fileMock.js"
    },
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).ts?(x)"
    ],
    "transform": {
      "\\.(js|jsx)$": "babel-jest",
      "\\.(ts|tsx)$": "babel-jest"
    },
    "setupFiles": [
      "<rootDir>/util/setupTests.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/__tests__/**"
    ]
  }
}
